{
  "name": "json2array",
  "version": "2.3.6",
  "description": "Transforms JS objects to arrays",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src --fix",
    "build": "babel src/ -d lib/",
    "test": "jest",
    "precommit": "lint-staged && yarn prepare",
    "contrib": "./scripts/contrib.sh",
    "changelog":
      "github-changes -o holvonix-open -r json2array -n latest --order-semver --use-commit-body && prettier --write CHANGELOG.md",
    "tidy": "yarn contrib && yarn changelog",
    "changelog-ci":
      "github-changes -o holvonix-open -r json2array -n latest --order-semver --use-commit-body --token $GH_RO_TOKEN && prettier --write CHANGELOG.md",
    "tidy-ci": "yarn contrib && yarn changelog-ci",
    "prepare": "yarn lint && yarn build && yarn test"
  },
  "engines": {
    "node": ">=4.0.0",
    "yarn": "^1.3.2"
  },
  "keywords": ["csv", "json", "mapping", "transform", "arrays"],
  "lint-staged": {
    "src/**/*.{js,jsx}": ["prettier --parser flow --write", "git add"],
    "**/*.{json,md}": ["prettier --write", "git add"]
  },
  "homepage": "https://github.com/holvonix-open/json2array#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holvonix-open/json2array.git"
  },
  "author":
    "Holvonix LLC and the json2array AUTHORS <sourcecode@holvonix.com> (https://github.com/holvonix-open/json2array)",
  "contributors": ["See AUTHORS file", "See CONTRIBUTORS file"],
  "files": [
    "lib/",
    "CHANGELOG.md",
    "package.json",
    "LICENSE",
    "AUTHORS",
    "NOTICE",
    "README.md",
    "CONTRIBUTORS",
    ".flowconfig",
    ".babelrc",
    ".eslintrc.json",
    ".npmignore",
    ".gitignore",
    ".travis.yml",
    "yarn.lock"
  ],
  "license": "Apache-2.0",
  "private": false,
  "devDependencies": {
    "js-devbox": "^1.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage/"
  }
}
